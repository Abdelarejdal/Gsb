                        <th><h2>Mémoire externe</h2></th>
                        <th><h2>Couleur</h2></th>
                        <th><h2>Modele</h2></th>
                        
                      
                      
                      
                        
                     </tr>
                   
                    {% set boucle = boucle + 1 %} 
                  
	         <tr><td><h2>{{ N}}</h2></td>
                <td><h2>{{ P}}</h2></td>
                <td><h2>{{ T}}</h2></td>
                <td><h2>{{ I}}</h2></td>
                <td><h2>{{ E}}</h2></td>
                <td><h2>{{ C}}</h2></td>
                <td><h2>{{ M}}</h2></td>
                

             </tr>
             
             {% endif %}
            
             {%endfor%}
             {%endfor%}
             {%endfor%}
             {%endfor%}
             {%endfor%}
              {%endfor%}
             {%endfor%}

</table></center>
